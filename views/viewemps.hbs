  <h2>View All Employee</h2>

<table class="table table-hover">
    <tr><th>EID</th><th>Name</th><th>Salary</th><th>Address</th></tr>

{{#each data}}
<tr> <td>{{this.eid}}</td>
      <td>{{this.ename}}</td>
      <td>{{this.salary}}</td>
      <td>{{this.address}}</td>
      <td><a href="/DeleteEmp?empid={{this.eid}}"
    onClick="return confirm('Deleted????')"
        class="glyphicon glyphicon-trash"></a></td>
        <td><a href="/ShowEmp?empid={{this.eid}}"
          class="glyphicon glyphicon-edit"></a></td>
</tr>
{{/each}}


</table>



  </div>
